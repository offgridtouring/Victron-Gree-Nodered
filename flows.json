        "type": "function",
        "z": "3d65881ca5c4117b",
        "name": "Temp Feedback",
        "func": "// Check if payload is an object with a value, or just a raw number\nlet temp = (typeof msg.payload === 'object') ? msg.payload.value : msg.payload;\n\n// Check if temp is valid, otherwise stop here\nif (temp === undefined || temp === null) {\n    node.warn(\"Temperature is undefined!\");\n    return null; \n}\n\n// Format for the Victron Output Node\nmsg.payload = {\n    \"/SwitchableOutput/output_1/Dimming\": Number(temp)\n};\n\nreturn msg;",
